{
  "name": "davidjeddy/no-code",
  "description": "A super scalable, machine learning optimized, DevOps/CI/CD ready, 100% secure, block-chain compatible, micro-service architecture designed, quantum computing calculated, IOT deployable, zero performance degrading PHP package.",
  "homepage": "https://github.com/davidjeddy/no-code",
  "license": "MIT",
  "authors": [
    {"name": "David J Eddy", "email": "me@davidjeddy.com" }
  ],
  "repositories": [
    {
      "type":"package",
      "package": {
        "name": "kelseyhightower/nocode",
        "version":"1.0",
        "source": {
          "url": "https://github.com/kelseyhightower/nocode.git",
          "type": "git",
          "reference":"master"
        }
      }
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "kelseyhightower/nocode": "^1.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "phpunit/phpunit": "^6",
    "sebastian/phpcpd": "^3.0",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "NoCode\\": ["src/no-code"]
    }
  },
  "autoload-dev": {
    "psr-4": {"NoCode\\": "tests/no-code"}
  }
}
